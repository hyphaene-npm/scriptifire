{
  "name": "scriptifire",
  "version": "0.0.0",
  "description": "a cli to generate usefull scripts",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "patch": "versionifier patch",
    "minor": "versionifier minor",
    "major": "versionifier major"
  },
  "bin": {
    "scriptifire": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyphaene/scriptifire.git"
  },
  "keywords": [
    "scripts",
    "package.json"
  ],
  "author": "maximilien garenne <maximilien.garenne@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyphaene/scriptifire/issues"
  },
  "homepage": "https://github.com/hyphaene/scriptifire#readme",
  "dependencies": {
    "fs": "^0.0.2",
    "fs-extra": "^8.1.0",
    "global": "^4.3.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "versionifier": "^1.3.4"
  },
  "versionifier": {
    "commitIfOnlyPackageJsonInStage": true,
    "commitIfMultipleFilesInStage": false,
    "push": true,
    "commitMessage": "Updated package.json to version @@",
    "remoteRepo": "origin",
    "publish": true
  }
}
